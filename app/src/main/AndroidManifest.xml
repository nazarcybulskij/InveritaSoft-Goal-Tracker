<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="nazarko.inveritasoft.com.inveritasoft_goal_tracker">

    <application
        android:name=".HabitsApplication"
        android:allowBackup="true"
        android:icon="@drawable/ic_goal"
        android:label=""
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:fullBackupContent="false">
        <activity android:name=".ui.main.MainActivity"
            android:theme="@style/HabbitActivityStyle">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".ui.main.createUIhabbits.CreateHabbitActivity"
            android:theme="@style/CreateHabbitActivityStyle"
            />

    </application>

</manifest>